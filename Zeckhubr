local HookEnabled = false
local mt = getrawmetatable(game)
setreadonly(mt, false)
local oldNamecall = mt.__namecall

local function HookFunction(self, ...)
    local args = {...}
    local method = getnamecallmethod()

    if HookEnabled and self == Interact and method == sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'*************************************************************************','**************************************************************************************************************','**********************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','***********************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}) and typeof(args[1]) == sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) then
        local data = args[1]
        if data.LookVector then data.LookVector = currentLookVector end
        if data.TiltDirection then data.TiltDirection = currentLookVector end
        if data.MoveDirection then data.MoveDirection = currentLookVector end
        return oldNamecall(self, data)
    end
    return oldNamecall(self, ...)
end

