local GameTab = Window:CreateTab(KjTynGEZqsgifgyLHxtdeZLKoBXmrRlSXdXRXTSyWTwcREQiHblvnkyRmW({'***********************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************'}), KjTynGEZqsgifgyLHxtdeZLKoBXmrRlSXdXRXTSyWTwcREQiHblvnkyRmW({'******************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************'}))
