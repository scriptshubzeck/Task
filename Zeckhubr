local CurrentHitboxScale = 5.0
local hitboxEnabled = true
local ballHitboxes = {} 


local function findAnyPart(model)
    for _, part in ipairs(model:GetDescendants()) do
        if part:IsA(sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'******************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'})) then
            return part
        end
    end
    return nil
end


local function createOrUpdateHitboxes(scale)
    for _, model in ipairs(Services.Workspace:GetChildren()) do
        if model:IsA(sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) and model.Name:match(sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'**********************************************************************************************','*******************************************************************','****************************************************************************','*************************************************************************','*********************************************************************','******************************************************************************','************************************************************************************','***********************************************************************************************','******************************************************************','*****************************************************************','****************************************************************************','****************************************************************************','***********************************************************************************************','*************************************','****************************************************************************************************','*******************************************','************************************'})) then
            local ball = model:FindFirstChild(sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'******************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**********************************************','************************************************','************************************************','*************************************************'}))
            if not ball then
                local ref = findAnyPart(model)
                if ref then
                    ball = Instance.new(sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
                    ball.Name = sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'******************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**********************************************','************************************************','************************************************','*************************************************'})
                    ball.Shape = Enum.PartType.Ball
                    ball.Size = Vector3.new(2, 2, 2) * scale
                    ball.CFrame = ref.CFrame
                    ball.Anchored = true
                    ball.CanCollide = false
                    ball.Transparency = 0.7
                    ball.Material = Enum.Material.ForceField
                    ball.Color = Color3.fromRGB(0, 255, 0)
                    ball.Parent = model
                    ballHitboxes[model] = ball
                end
            else
                ball.Size = Vector3.new(2, 2, 2) * scale
            end
        end
    end
end
local function clearHitboxes()
    for model, ball in pairs(ballHitboxes) do
        if ball and ball.Parent then
            ball:Destroy()
        end
    end
    ballHitboxes = {}
end

local ballAddedConnection
ballAddedConnection = Services.Workspace.ChildAdded:Connect(function(child)
    if child:IsA(sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) and child.Name:match(sghzHjrPqaWmqxCGunkvTaMGopRhTXydwP({'**********************************************************************************************','*******************************************************************','****************************************************************************','*************************************************************************','*********************************************************************','******************************************************************************','************************************************************************************','***********************************************************************************************','******************************************************************','*****************************************************************','****************************************************************************','****************************************************************************','***********************************************************************************************','*************************************','****************************************************************************************************','*******************************************','************************************'})) then
        if hitboxEnabled then
            task.wait(0.1)
            createOrUpdateHitboxes(CurrentHitboxScale)
        end
    end
end)

