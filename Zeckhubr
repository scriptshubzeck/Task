    Name = KjTynGEZqsgifgyLHxtdeZLKoBXmrRlSXdXRXTSyWTwcREQiHblvnkyRmW({'****************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************************************************','************************************************************************************************************','********************************','********************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************'}),
    CurrentValue = false,
    Flag = KjTynGEZqsgifgyLHxtdeZLKoBXmrRlSXdXRXTSyWTwcREQiHblvnkyRmW({'********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************','*********************************************************************************************************','********************************************************************************************************************','************************************************************************************','***************************************************************************************************************','*******************************************************************************************************','*******************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}),
    Callback = function(value)
        HookEnabled = value
        if value then
            mt.__namecall = newcclosure(HookFunction)
        else
            mt.__namecall = oldNamecall
        end
    end
})


local serveEnabled = false
local servePower = 1

mt.__namecall = newcclosure(function(self, ...)
    local args = {...}
    local method = getnamecallmethod()

    if tostring(self) == tostring(Serve) and method == KjTynGEZqsgifgyLHxtdeZLKoBXmrRlSXdXRXTSyWTwcREQiHblvnkyRmW({'*************************************************************************','**************************************************************************************************************','**********************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','***********************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}) then
        if serveEnabled then
            args[2] = servePower
            return oldNamecall(self, unpack(args))
        end
    end

    return oldNamecall(self, ...)
end)

