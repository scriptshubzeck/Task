    mobileTiltButton = Instance.new(KjTynGEZqsgifgyLHxtdeZLKoBXmrRlSXdXRXTSyWTwcREQiHblvnkyRmW({'************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','********************************************************************************************************************','******************************************************************','*********************************************************************************************************************','********************************************************************************************************************','********************************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}))
